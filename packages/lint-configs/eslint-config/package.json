{
  "name": "@fireflymit/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "ESLint config for fireflymit",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "tsc --watch",
    "build": "unbuild",
    "build:out": "unbuild --out",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.34.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.2"
  }
}
